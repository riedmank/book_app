<!DOCTYPE html>
<html>
  <%- include('partials/head') %>
  <%- include('partials/header') %>
  <body>
    <div class="card">
      <form action="/books" method="POST" id="newBook">
        <label>Title<input type="text" name="title" required /></label>
        <label>Author<input type="text" name="author" required /></label>
        <label>ISBN<input type="text" name="isbn" required /></label>
        <label>Image_URL<input type="text" name="image_url" required /></label>
        <label>Description<textarea form="newBook" name="description" required></label>
        <input type="submit">
      </form>
    </div>
  </body>
</html>